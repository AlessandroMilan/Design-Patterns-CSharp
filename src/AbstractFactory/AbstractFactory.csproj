    <AssemblyName>AbstractFactory</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>AbstractFactory</PackageId>
    <RuntimeFrameworkVersion>1.1.4</RuntimeFrameworkVersion>
